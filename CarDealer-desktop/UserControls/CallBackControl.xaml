<UserControl x:Class="CarDealer.UserControls.CallBackControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CarDealer.UserControls"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="500">
    <Border BorderThickness="1" Margin="10" BorderBrush="#05ace2">
        <Grid>
            <StackPanel Width="auto" HorizontalAlignment="Center" VerticalAlignment="Center">
                <Label x:Name="CountOrder_Label"/>
                <Label x:Name="Status_Label"/>
                <Label x:Name="DateOrder_Label"/> 
                <Label Name="isManager_Label"/>
            </StackPanel>
        </Grid>
    </Border>
</UserControl>
